#%RAML 0.8
---
title: example
baseUri: http://nidi.guru/raml/{version}
version: v1
schemas:
  - string: !include string.json

/ok:
  post:
    body:
      application/json:
        schema: |
          {"type":"string"}
        example: |
          "42"
/okref:
  post:
    body:
      application/json:
        schema: string
        example: |
          "42"

/nok:
  post:
    body:
      application/json:
        schema: string
        example: 42
    responses:
      200:
        body:
          application/json:
            schema: string
            example: 42
