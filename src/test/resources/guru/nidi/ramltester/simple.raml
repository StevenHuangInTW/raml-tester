#%RAML 0.8
---
title: simple
baseUri: http://guru.nidi/raml/simple/{version}
version: v1
mediaType: application/default
schemas:
  - string: !include string.json

/data:
  get:
    responses:
      200:
        body:
          application/json:
            schema: |
              {"type":"string"}
/query:
  get:
    queryParameters:
      req:
        required: true
      rep:
        repeat: true
    responses:
      200:
        body:
          application/json:
            schema: |
              {"type":"string"}
/schema:
  get:
    responses:
      200:
        body:
          application/json:
            schema: |
              {"type":"string"}
      201:
        body:
          application/json:
            schema: !include string.json
      202:
        body:
          application/json:
            schema: string
      203:
        body:
          application/json:
            schema: undefined
/mediaType:
  get:
    responses:
      200:
        body:
          schema: string

