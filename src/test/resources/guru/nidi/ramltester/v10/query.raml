#%RAML 1.0
---
title: query
baseUri: http://nidi.guru/raml/{version}
version: v1
mediaType: application/default
schemas:
  - string: !include string.json

/data:
  get:
    responses:
      200:
        body:
          application/json:
            schema: |
              {"type":"string"}
/query:
  get:
    queryParameters:
      req:
        required: true
      rep:
        repeat: true
      int:
        type: integer
    responses:
      200:
        body:
          application/json:
            schema: |
              {"type":"string"}
